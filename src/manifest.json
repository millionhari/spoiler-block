{
  "manifest_version": 2,

  "name": "Spoiler Block",
  "description": "This extension will black out divs based on specific keywords",
  "version": "1.0",
  "permissions": ["storage"],

  "content_scripts": [
     {
       "matches": ["https://*/*", "http://*/*"],
       "css": ["/styles/style.css"],
       "js": ["/scripts/app.js"]
     }
   ]
}
